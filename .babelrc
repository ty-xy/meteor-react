{
    "presets": [
        "stage-0", [
            "env",
            {
                "targets": {
                    "browsers": ["last 3 versions"]
                },
                "modules": false
            }
        ]
    ],
    "plugins": [
        "transform-runtime",
        "transform-decorators-legacy",
        "transform-class-properties"
    ],
    "env": {
        "development": {
            "plugins": [
                [
                    "react-transform",
                    {
                        "transforms": [{
                                "transform": "react-transform-style"
                            },
                            {
                                "transform": "react-transform-log-render"
                            }
                        ]
                    }
                ]
            ]
        }
    }
}